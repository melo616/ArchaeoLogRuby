<p style="color: green"><%= notice %></p>

<h1>Artifacts</h1>

<div id="artifacts">
  <% @artifacts.each do |artifact| %>
    <%= render artifact %>
    <p>
      <%= link_to "Show this artifact", dig_artifact_path(@dig.id, artifact.id) %>
    </p>
  <% end %>
</div>

<%= link_to "New artifact", new_dig_artifact_path %>
